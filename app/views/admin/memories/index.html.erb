<table class="table table-striped">
  <thead>
    <td><h1>Memories</h1></td>
      <tbody>
        <% @memories.each do |memory| %>
        <tr>
          <td><%= memory.question.question %></td>
          <td><%= memory.question.category.name %></td>
          <td><%= memory.body %></td>
          <td><%= memory.year %></td>
          <td><%= memory.season %></td>
          <td><%= memory.user.full_name %></td>
          <td><%= button_to "Remove", admin_memory_path(memory), method: :delete, data: {confirm: "Are you sure?" } %>
        </tr>
        <% end %>
      <tbody>
        <%= link_to "Admin Home", admin_root_path %>
</table>