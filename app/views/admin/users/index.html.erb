<table class="table table-striped">
  <thead>
    <td><h1>Users</h1></td>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
          <td><%= user.birth_month %></td>
          <td><%= user.birth_day %></td>
          <td><%= user.birth_year %></td>
          <td><%= user.memories.count %></td>
          <td><%= link_to "Edit", edit_user_registration_path(user) %></td>
          <td><%= link_to "Remove", user, method: :delete, data: {confirm: "Are you sure?" } %>
        </tr>
        <% end %>
      <tbody>
</table>