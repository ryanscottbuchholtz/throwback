  <%= csrf_meta_tags %>
</head>
<body>

<div class="container-narrow">
  <div class="masthead">
    <%= render 'layouts/user_header' %>
    <h4 class="muted"><%= link_to 'throwback', root_path %></h4>
      <h5 class='muted'>your story, your way</h5>
  </div>
  <hr />
  
  <p class="notice"><%= notice %></p>
